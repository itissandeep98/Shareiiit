# /etc/nginx/sites-available/share.osa.iiitd.edu.in

server {
	listen 80;
	server_name share.osa.iiitd.edu.in;
	
	location = /favicon.ico { access_log off; log_not_found off; }		
	
	#location /static {
	#	root /home/iiitd/share-iiitd/backend;
	#}
	
	location /api {
		include proxy_params;
		proxy_pass http://unix:/run/share-iiitd.sock;
	}

	location / {
		root /var/www/share.osa.iiitd.edu.in;
                try_files $uri /index.html ;
        }
}
